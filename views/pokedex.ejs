<%- include('partials/header') %> 
  <h1 class="my-title">Poke-Dex</h1>

  <div class="container">
    <div class="row">
      <div class="col-sm">
        <table class="table table-dark">
          <thead>
            <tr>
              <th scope="col">Name</th>
            </tr>
          </thead>
          <tbody>
            <% pokemons.forEach(element => { %>
              <tr class="my-row">
                <th scope="row"> <%- element.name %>  </th>
              </tr>
            <% }) %>
          </tbody>
        </table>
      </div>
      <div class="col-sm">
        <div class="img-post">
          <h4 id="pokemon-name">Pokemon Name</h4>
          <img src="https://images.tokopedia.net/img/cache/500-square/product-1/2020/3/7/96536707/96536707_3e793bcf-e334-4947-8449-d5633fdfdc03_338_337.jpg" alt="" class="pokemon-img" id="pokemon-img">
          <h2><a id="pokemon-detail-link" href="#">See Detail</a></h2>
        </div>
      </div>
    </div>
  </div>

  <script src="https://code.jquery.com/jquery-3.6.0.js" integrity="sha256-H+K7U5CnXl1h5ywQfKtSj8PCmoN9aaq30gDh27Xc0jk=" crossorigin="anonymous"></script>
  <script src="js/script.js"></script>
</body>
</html>